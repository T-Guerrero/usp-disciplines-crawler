{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es2021": true
  },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
      ],
      "rules": {
        "prettier/prettier": "error"
      }
}